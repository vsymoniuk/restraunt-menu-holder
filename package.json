{
  "name": "owp",
  "version": "1.0.0",
  "description": "project on nodejs and angular",
  "main": "index.js",
  "engines": {
    "node": "10.16.3",
    "npm": "6.9.0"
  },
  "scripts": {
    "start": "node index",
    "server": "nodemon index",
    "client-install": "npm install --prefix owp-client",
    "client": "npm run start --prefix owp-client",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run build --prefix owp-client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vsymoniuk/owp.git"
  },
  "keywords": [
    "nodejs",
    "angular",
    "course",
    "work"
  ],
  "author": "vsymoniuk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vsymoniuk/owp/issues"
  },
  "homepage": "https://github.com/vsymoniuk/owp#readme",
  "dependencies": {
    "@angular/http": "^7.2.15",
    "@cloudinary/angular": "^2.1.1",
    "@cloudinary/angular-5.x": "^1.0.3",
    "@sendgrid/mail": "^6.4.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.16.0",
    "cloudinary-core": "^2.7.4",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "datauri": "^2.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "node-telegram-bot-api": "^0.40.0",
    "nodemon": "^1.19.3",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  }
}
